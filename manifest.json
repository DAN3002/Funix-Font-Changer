  {
    "name": "FUXiX Font Changer",
    "version": "1.1",
    "description": "Custom setting for subtitle in LMS",
    "manifest_version": 2,
	"content_scripts": 
	[
	   {
	     "matches": ["https://lms.funix.edu.vn/*", "https://lagunita.stanford.edu/*"],
	     "js": [
	     	"JS/jquery-3.3.1.min.js",	     	
	     	"JS/content.js"     	  	
	     ]
	   }
	],
    "permissions": [
      "tabs"
    ],	   
	"browser_action":
	{
		"default_icon": "Icon/icon3.png",
		"default_title": "FUNiX Font Changer",
		"default_popup": "Popup/index.html"
	},
	"icons": { 
		"16": "Icon/icon1.png",
		"48": "Icon/icon2.png",
		"128": "Icon/icon3.png" 
	}
  }